        <script><![CDATA[var lemsAjax = Class.create();
lemsAjax.prototype = Object.extendsObject(AbstractAjaxProcessor, {
    getDateDiff: function() {
        var startDate = this.getParameter('sysparam_start');
        var endDate = this.getParameter('sysparam_end');

        if (!startDate || !endDate) {
            return 0; // Return 0 if any date is missing
        }

        var d1 = new GlideDateTime(startDate);
        var d2 = new GlideDateTime(endDate);

        var durationMillis = d2.getNumericValue() - d1.getNumericValue();
        var durationDays = Math.abs(durationMillis / (1000 * 60 * 60 * 24));

        return Math.floor(durationDays); 
    }
});
]]></script>
